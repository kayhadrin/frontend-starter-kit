{
  "name": "frontend-starter-kit",
  "version": "1.0.0",
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx,scss}": [
      "prettier --write"
    ]
  },
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "engines": {
    "node": "v22"
  },
  "scripts": {
    "dev": "WATCHPACK_POLLING=750 next dev",
    "format": "prettier --write .",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --concurrency auto --cache --max-warnings=0",
    "lint:format": "prettier --cache --check .",
    "lint-n-test": "npm-run-all --parallel lint:* test:*",
    "test": "npm-run-all --parallel test:*",
    "//test:jest": "pnpm jest",
    "test:types": "tsc",
    "postinstall": "next telemetry disable"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "next": "^15.5.4",
    "nullthrows": "^1.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@tailwindcss/postcss": "^4",
    "@types/invariant": "^2.2.37",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.38.0"
  }
}
